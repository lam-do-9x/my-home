datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

model Dictionary {
    id              Int      @id @default (autoincrement())
    word            String   @unique
    ipa             String?
    pronunciation   String?
    content         String?
    createdAt       DateTime? @default (now())
    updatedAt       DateTime? @updatedAt
}
